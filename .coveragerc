[run]
source = calculator
omit =
    */tests/*
    */migrations/*
    */__init__.py
    */settings.py
    */manage.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
    except ImportError:
    def main
show_missing = True
fail_under = 80
